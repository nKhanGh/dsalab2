void push(T item) {
    list.add(item);
}

T pop() {
    // TODO: Remove an element on top of the stack
    T c = list.removeAt(list.size() - 1);
    return c;
}

T top() {
    // TODO: Get value of the element on top of the stack
     return list.get(list.size() -1);   
}

bool empty() {
    return list.size() == 0;  
}

int size() {
    return list.size();  
}

void clear() {
    // TODO: Clear all elements of the stack
    list.clear();
}

bool isValidParentheses (string s){
    stack<char> st;
    for(size_t i = 0; i < s.size(); i++){
        if(st.empty()) st.push(s[i]);
        else{
            if(st.top() == '(' && s[i] == ')') st.pop();
            else if(st.top() == '[' && s[i] == ']') st.pop();
            else if(st.top() == '{' && s[i] == '}') st.pop();
            else st.push(s[i]);
        }
    }
    return st.empty();
}
